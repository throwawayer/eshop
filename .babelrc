{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "extensions": [".tsx"],
                "root": ["./src"]
            }
        ],
        "@babel/plugin-syntax-dynamic-import",
        "react-hot-loader/babel"
    ]
}
